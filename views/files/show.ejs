<% locals.breadcrumbs = [
    {name: 'Home', link: '/'},
    {
        name: file.capture.experiment.sample.project.group.name,
        link: `/browse/${file.capture.experiment.sample.project.group.safeName}`
    },
    {
        name: file.capture.experiment.sample.project.name,
        link: `/browse/${file.capture.experiment.sample.project.group.safeName}/${file.capture.experiment.sample.project.safeName}`
    },
    {
        name: file.capture.experiment.sample.name,
        link: `/browse/${file.capture.experiment.sample.project.group.safeName}/${file.capture.experiment.sample.project.safeName}/${file.capture.experiment.sample.safeName}`
    },
    {
        name: file.capture.experiment.name,
        link: `/browse/${file.capture.experiment.sample.project.group.safeName}/${file.capture.experiment.sample.project.safeName}/${file.capture.experiment.sample.safeName}/${file.capture.experiment.safeName}`
    },
    {name: file.name, active: true}
]; %>
<% include ../head.ejs %>
<section class="section">
    <div class="container">

        <h1 class="title has-text-centered"><%- capture.name %></h1>
        <% const HPCPath = `${locals.config.HPCRoot}/${file.capture.experiment.sample.project.group.safeName}/${file.capture.experiment.sample.project.safeName}/${file.capture.experiment.sample.safeName}/${file.capture.experiment.safeName}/${file.capture.safeName}/${file.safeName}` %>
        <% include ../_hpc_path.ejs %>
        <hr>
        <div class="columns">
            <div class="column">
                <label class="label">Name</label>
                <p><%- file.originalName %></p>

                <label class="label">Type</label>
                <p><%- file.type %></p>

                <% if(file.description){ %>
                    <label class="label">Description</label>
                    <p><%- file.description %></p>
                <% } %>
            </div>
        </div>


    </div>
</section>
<% include ../foot.ejs %>